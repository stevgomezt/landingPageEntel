{
    "rewrites": [
        { "source": "/", "destination": "/index.html" },
        { "source": "/mapa_fija", "destination": "/mapa_fija.html" }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, must-revalidate"
                }
            ]
        }
    ]
}
